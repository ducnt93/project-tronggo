@model ShopTrongGo.Models.TaiKhoan
@{
    ViewBag.Title = "Quên mật khẩu";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="top">
    <div class="left"></div>
    <div class="center">
        <h1>Cấp lại mật khẩu</h1>
    </div>
</div>
<div class="middle" style="padding:0px;">
    <div class="list">
        @using (Html.BeginForm("ForgotPass","Account",FormMethod.Post))
        {
            @Html.AntiForgeryToken()
            @Html.ValidationSummary(true)

            <fieldset>
                <legend>Form</legend>
                Email: @Html.TextBoxFor(m => Model.Email)
                <input type="submit" value="Gửi" />
            </fieldset>

        }
    </div>
</div>
