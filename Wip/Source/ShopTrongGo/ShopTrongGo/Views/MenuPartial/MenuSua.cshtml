@using ShopTrongGo.Models
<div class="box">
    <div class="top">Sữa</div>
    <div id="category" class="middle">
        <ul>
            @{
                var list = ((List<LoaiSanPham>)ViewBag.MenuSua);
                if (list != null)
                {
                    foreach (var item in list)
                    {
                        <li><a href="~/Product/All/@item.LoaiSpID">@item.TenLoaiSp</a></li>
                    }
                }
            }
        </ul>
    </div>
</div>