@using ShopTrongGo.Models
<div class="box">
    <div class="top">Dịch vụ</div>
    <div id="category" class="middle">
        <ul>
            @{
                var list = ((List<DichVu>)ViewBag.MenuService);
                if (list != null)
                {
                    foreach (var item in list)
                    {
                        <li>
                            @Html.ActionLink(@item.TenDichVu, "DetailNews", "News", new { id = @item.DichVuID, @item.TenKhongDau }, null)
                        </li>
                    }
                }
            }
        </ul>
    </div>
</div>